##### GENERAL SETTINGS
# User and group IDs for Docker to set file permissions correctly
PUID=uid_here
PGID=gid_here
# Timezone
TZ="Your/Timezone"

##### DIRECTORIES
# Base directory paths for volumes and data
HOMEDIR="/path/to/home/"
USERDIR="/path/to/userdir/"
DOCKERDIR="/path/to/docker/"
DATADIR="/path/to/data/"
NEXTCLOUDDIR="/path/to/nextcloud/"
SECRETSDIR="/path/to/docker/secrets/"
PHOTOPRISMDIR="/path/to/photos/"

##### NETWORK CONFIGURATION
# Server IP address on your local network
SERVER_IP=local_ip_here
# Subnet configuration for your local network
LOCAL_NETWORK=local_subnet_here
# Network range for Traefik
TRAEFIK_NETWORK=traefik_network_here

##### DOMAIN INFORMATION
# Primary and secondary domain names
DOMAINNAME0=your_domain_name_here
DOMAINNAME1=your_backup_domain_name_here
# Cloudflare Zone ID
CLOUDFLARE_ZONEID=your_cloudflare_zone_id_here
# Traefik Pilot Token
TRAEFIK_PILOT_TOKEN=your_traefik_pilot_token_here
# Domain names for specific services
NEXTCLOUD_DOMAIN_NAME=nextcloud.your_domain_name_here
#PHOTOPRISM_DOMAIN_NAME=photoprism.your_domain_name_here

##### EMAIL SETTINGS
# Email used for SMTP and notifications
SMTP_EMAIL=your_email_here
# SMTP password
SMTP_PASS=your_smtp_password_here

##### DATABASE SETTINGS
# Database host and port (if needed)
# DB_HOST=db_host_here
# DB_PORT=db_port_here
# MySQL root password
MYSQL_ROOT_PASSWORD=your_mysql_root_password_here

##### API KEYS
# API keys for various services
LIDARR_API_KEY=your_lidarr_api_key_here
RADARR_API_KEY=your_radarr_api_key_here
READARR_API_KEY=your_readarr_api_key_here
SONARR_API_KEY=your_sonarr_api_key_here
BAZARR_API_KEY=your_bazarr_api_key_here
PROWLARR_API_KEY=your_prowlarr_api_key_here
JACKETT_API_KEY=your_jackett_api_key_here
NOTIFIARR_API_KEY=your_notifiarr_api_key_here
SABNZBD_API_KEY=your_sabnzbd_api_key_here
PORTAINER_API_KEY=your_portainer_api_key_here

##### NOTIFICATION SETTINGS
# Telegram bot token and chat ID for notifications
# TGRAM_BOT_TOKEN=your_telegram_bot_token_here
# TGRAM_CHAT_ID=your_telegram_chat_id_here
# Email for notifications
# MY_EMAIL=your_notification_email_here

##### APPLICATION CREDENTIALS
# VPN credentials if used
CYBERGHOST_USERNAME=your_cyberghost_username_here
CYBERGHOST_PASSWORD=your_cyberghost_password_here
# Transmission RPC credentials
# TRANSMISSION_RPC_USERNAME=your_rpc_username_here
# TRANSMISSION_RPC_PASSWORD=your_rpc_password_here
# VNC passwords for various services
HANDBRAKE_VNC_PASSWD=your_vnc_password_here
QDIRSTAT_VNC_PASSWD=your_vnc_password_here
# Redis and VSCode passwords
REDIS_PASSWORD=your_redis_password_here
VSCODE_PASSWORD=your_vscode_password_here

##### PLEX INTEGRATION
# Plex access tokens and server IPs (specify for different setups)
# SYN_PLEX=your_plex_access_token_here
# NUC_PLEX=your_plex_access_token_here
# Media library numbers for different Plex servers
# SYN_PLEX_HOLLYWOOD=library_number_here
# NUC_PLEX_HOLLYWOOD=library_number_here
# SYN_PLEX_TVSHOWS=library_number_here
# NUC_PLEX_TVSHOWS=library_number_here

